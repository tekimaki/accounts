plugin services:
  - index:
    - content_page_<page>_func
	- content_page_<page>.tpl
  - admin
    - content_admin_func

htaccess rewrites:
  - rewrite account/<name> to account/index.php?account_name=<name> 
  - rewrite account/id/<id> to account/index.php?account_id=<id>
  - rewrite account/cid/<cid> to account/index.php?account_content_id=<cid>
  - rewrite account/<name>/<page>/ to account/index.php?account_name=<name>&page=<page>
  - rewrite account/id/<id>/<page>/ to account/index.php?account_id=<id>&page=<page>
  - rewrite account/cid/<cid>/<page>/ to account/index.php?account_content_id=<cid>&page=<page>

